    </servlet>
    <servlet>
        <servlet-name>AddAddrServlet</servlet-name>
        <servlet-class>pl.jeeweb.zadanie23.servlet.AddAddrServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DelAddrServlet</servlet-name>
        <servlet-class>pl.jeeweb.zadanie23.servlet.DelAddrServlet</servlet-class>
