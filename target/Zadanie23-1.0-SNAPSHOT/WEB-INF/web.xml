    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>pl.jeeweb.zadanie23.servlet.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>pl.jeeweb.zadanie23.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
