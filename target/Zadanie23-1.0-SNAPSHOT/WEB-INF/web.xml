    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PremiumPrivServlet</servlet-name>
        <url-pattern>/PremiumPrivServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddAddrServlet</servlet-name>
        <url-pattern>/AddAddrServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DelAddrServlet</servlet-name>
        <url-pattern>/DelAddrServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditAddrServlet</servlet-name>
        <url-pattern>/EditAddrServlet</url-pattern>
    </servlet-mapping>
